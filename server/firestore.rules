rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // Block ALL client-side access
    // DevTrack uses Firebase Admin SDK on the server, 
    // which bypasses these rules.
    // All data access goes through the Express API with Clerk auth.
    match /{document=**} {
      allow read, write: if false;
    }
  }
}
